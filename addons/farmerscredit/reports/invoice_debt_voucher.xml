        <record id="action_invoice_debt_voucher" model="ir.actions.report">
            <field name="name">Invoice Debt Voucher</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">farmerscredit.invoice_debt_voucher</field>
