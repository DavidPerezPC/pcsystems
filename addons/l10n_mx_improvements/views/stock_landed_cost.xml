<odoo>
  <data>
    <record id="res_partner_improvements_inherit_form" model="ir.ui.view">
      <field name="name">res_partner_improvements_inherit_form</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="l10n_mx_edi_40.l10n_mx_edi_inh_40_res_partner_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='l10n_mx_edi_fiscal_regime']" position="after">
          <field name="l10n_mx_edi_usage" />
          <field name="l10n_mx_edi_payment_method_id" />
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="view_stock_landed_cost_form_l10n_mx_stock_improved">
        <field name="name">view.stock.landed.cost.form.l10n_mx_stock.improved</field>
        <field name="model">stock.landed.cost</field>
        <field name="inherit_id" ref="l10n_mx_edi_landing.view_stock_landed_cost_form_l10n_mx_stock"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='l10n_mx_edi_customs_number']" position="after">
                <field name="customs_date"/>
                <field name="customs_name"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>